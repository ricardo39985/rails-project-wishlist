<% if @dealerships.size > 0 %>
  <h3><%=params[:car][:dealership] %> cars are sold at the following dealerships</h3>
  <% @dealerships.each do |d| %>
    <div class="row valign-wrapper">
      <div class="col s12 ">
        <div class="card small hoverable">
          <div class="card-content grey-text darken-3">
            <h4><%=d.name%></h4>
            <h4><%=d.location%></h4>
          </div>
        </div>
      </div>
    </div>
  <% end %>
<% else %>
  <h3><%=params[:car][:dealership] %> cars not found at any dealerships</h3>

<% end %>
